    "ManifestBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {}
    },
    "S3Writer": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "DeploymentBucketName"
          },
          "S3Key": "LambdaSharp/Modules/S3.IO/Assets/S3Writer-DRYRUN.zip"
