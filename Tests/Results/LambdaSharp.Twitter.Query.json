    "LambdaSharp::Manifest": {
      "Version": "2018-12-31",
      "Module": "LambdaSharp.Twitter.Query:0.5-WIP",
      "ParameterSections": [
        {
          "Title": "Twitter Settings",
          "Parameters": [
            {
